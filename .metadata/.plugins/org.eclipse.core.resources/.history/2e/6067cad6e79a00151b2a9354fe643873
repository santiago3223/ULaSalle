#include <fstream>
#include<iostream>
#include <stdlib.h>
#include <stdio.h>
#include <math.h>
#include "BTreeNode.h"
#include "BTree.h"
using namespace std;

int main()
{
    BTree<int> t(5);
    for(int i =1 ;i <= 5; i++){
    	t.insertar(i,i+5);
    }

    t.printDot("archivo.dot");
	system("dot -Tpng archivo.dot -o archivo.png");
    cout<<"fin"<<endl;

    return 0;
}
