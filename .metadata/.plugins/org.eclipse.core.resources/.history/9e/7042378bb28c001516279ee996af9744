#include <iostream>

using namespace std;

template<class T>
class Pagina{
	public:
		Pagina<T>** paginas;
		int sizeAlfabeto;
		T * obj;
	Pagina(){

	}
	Pagina(int size){
		this->sizeAlfabeto = size;
		*obj = 0;
		paginas = new Pagina*[size];
		for(int i = 0; i< size; i++){
			paginas[i] = 0;
		}
	}


};

template<class T>
class Digital
{
	public:
	Pagina<T>* raiz;
	int size;
	Digital(int size){
		this->size = size;
		raiz = 0;
	}

	void Add(T dato){
		if(!raiz){
			raiz = new Pagina<T>(size);
		}
		Pagina<T> * q = raiz;
		for(int i = 0; i < strlen(dato);i++){
			Pagina * p = new Pagina<T>(size);
			q[(int)dato[0]-(int)'a'] = p;
			q = p;
		}

	}

	void Find(T dato){

	}

};



int main()
{
	Pagina<int> Pagina(10);
	cout<<"abb"<<endl;
	cout<< (int)'z'<<endl;


 return 0;
}
